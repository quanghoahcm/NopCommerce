@model IList<ProductOverviewModel>
+@using Nop.Web.Models.Catalog;
@{
    Layout = "";

    Html.AddScriptParts(ResourceLocation.Footer, "~/Plugins/Widgets.HomePageNewProducts/Scripts/slick.js");
    Html.AddScriptParts(ResourceLocation.Footer, "~/Plugins/Widgets.HomePageNewProducts/Scripts/custom.js");
    Html.AddCssFileParts("~/Plugins/Widgets.HomePageNewProducts/Content/slick.css");
    Html.AddCssFileParts("~/Plugins/Widgets.HomePageNewProducts/Content/slick-theme.css");
}

@if (Model.Count > 0)
{
<div class="product-grid home-page-product-grid">
    <div class="title">
        <strong>@T("New.Products")</strong>
    </div>
    <div class="item-grid center">
        @foreach (var product in Model)
        {
            <div class="item-box">
                @Html.Partial("_ProductBox", product)
            </div>
        }
    </div>
</div>
}